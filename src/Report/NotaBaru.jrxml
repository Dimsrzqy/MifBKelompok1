	<parameter name="IDTransaksiJual" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 
    tj.IDTransaksiJual,
    DATE_FORMAT(tj.TanggalJual, '%d %M %Y %H:%i:%s') AS TanggalTransaksi,
    tj.NamaUser AS Kasir,
    tj.MetodePembayaran,
    tj.TotalHarga,
    tj.Bayar,
    tj.Kembalian,
    tjd.NamaBarang,
    tjd.Kategori,
    tjd.Barcode,
    tjd.jumlah AS JumlahBarang,
    tjd.subtotal AS Subtotal,
    b.HargaJual AS HargaSatuan
FROM 
    transaksi_jual tj
JOIN 
    transaksi_jual_dtl tjd ON tj.IDTransaksiJual = tjd.IDTransaksiJual
JOIN 
    barang b ON tjd.IDBarang = b.IDBarang
WHERE tj.IDTransaksiJual = (
    SELECT IDTransaksiJual 
    FROM transaksi_jual 
    ORDER BY IDTransaksiJual DESC 
    LIMIT 1
)


ORDER BY 
    tjd.NamaBarang
]]>
	</queryString>
